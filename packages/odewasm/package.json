{
  "name": "odewasm",
  "version": "0.1.0",
  "description": "ODE solvers (DOPRI5, DOP853, RADAU5) compiled to WebAssembly",
  "type": "module",
  "main": "./dist/odewasm.cjs",
  "module": "./dist/odewasm.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/odewasm.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/odewasm.cjs"
      }
    },
    "./wasm/ode.wasm": "./dist/wasm/ode.wasm"
  },
  "scripts": {
    "build:wasm": "./scripts/build.sh",
    "build:lib": "vite build",
    "build": "pnpm run build:wasm && pnpm run build:lib",
    "test": "vitest run",
    "clean": "rm -rf dist/",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.2.0",
    "typedoc": "^0.27.0"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "ode",
    "differential-equations",
    "dopri5",
    "radau",
    "runge-kutta",
    "wasm",
    "webassembly"
  ],
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
