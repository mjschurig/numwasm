{
  "name": "lawasm",
  "version": "0.1.0",
  "description": "LAPACK + BLAS WebAssembly module for linear algebra",
  "type": "module",
  "main": "./dist/lawasm.cjs",
  "module": "./dist/lawasm.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/lawasm.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/lawasm.cjs"
      }
    },
    "./wasm/lapack.wasm": "./dist/wasm/lapack.wasm"
  },
  "scripts": {
    "build:wasm": "./scripts/build.sh",
    "build:lib": "vite build",
    "build": "pnpm run build:wasm && pnpm run build:lib",
    "test": "vitest run",
    "clean": "rm -rf dist/",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.2.0",
    "typedoc": "^0.27.0"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "lapack",
    "blas",
    "linear-algebra",
    "wasm",
    "webassembly",
    "fortran"
  ],
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
