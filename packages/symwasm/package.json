{
  "name": "symwasm",
  "version": "0.1.0",
  "description": "SymPy-inspired symbolic mathematics in TypeScript",
  "type": "module",
  "main": "./dist/symwasm.cjs",
  "module": "./dist/symwasm.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/symwasm.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/symwasm.cjs"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "clean": "rm -rf dist/",
    "docs": "typedoc",
    "lint": "eslint src tests --ext .ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typedoc": "^0.27.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.2.0"
  },
  "files": [
    "dist/"
  ],
  "keywords": [
    "sympy",
    "symbolic-math",
    "cas",
    "computer-algebra",
    "typescript"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false
}
